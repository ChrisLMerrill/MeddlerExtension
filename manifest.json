{
  "description": "Report HTTP traffic to the Meddler Gateway",
  "manifest_version": 2,
  "author": "Chris Merrill",
  "name": "Meddler",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "cookies",
    "<all_urls>"
  ],

  "applications": {
    "gecko": {
        "id": "Meddler-1.0@webperformance.com"
    }
},
  "background": {
    "scripts": ["axios.js", "messages.js", "http.js", "background.js"]
  }
}